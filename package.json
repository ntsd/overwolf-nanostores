{
  "name": "overwolf-nanostores",
  "version": "1.0.1",
  "description": "Nano Stores implement for Overwolf Plugin",
  "author": "Jirawat Boonkumnerd <contact@ntsd.dev> (https://ntsd.dev)",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "files": [
    "./dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc -b ./tsconfig.json",
    "docs": "typedoc ./src --out ./docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntsd/overwolf-nanostores.git"
  },
  "keywords": [
    "overwolf",
    "nanostores"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ntsd/overwolf-nanostores/issues"
  },
  "homepage": "https://github.com/ntsd/overwolf-nanostores#readme",
  "dependencies": {
    "nanostores": "^0.9.3"
  },
  "devDependencies": {
    "@overwolf/types": "^3.23.9",
    "typedoc": "^0.25.2"
  }
}